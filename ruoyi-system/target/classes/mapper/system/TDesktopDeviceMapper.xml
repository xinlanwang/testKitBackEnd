<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.TDesktopDeviceMapper">
    
    <resultMap type="TDesktopDevice" id="TDesktopDeviceResult">
        <result property="uid"    column="uid"    />
        <result property="carlineInfoUid"    column="carline_info_uid"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="createByUid"    column="create_by_uid"    />
        <result property="updateByUid"    column="update_by_uid"    />
        <result property="desktopDeviceInformation"    column="desktop_device_information"    />
    </resultMap>

    <sql id="selectTDesktopDeviceVo">
        select uid, carline_info_uid, create_time, update_time, create_by_uid, update_by_uid, desktop_device_information from t_desktop_device
    </sql>

    <select id="selectTDesktopDeviceList" parameterType="TDesktopDevice" resultMap="TDesktopDeviceResult">
        <include refid="selectTDesktopDeviceVo"/>
        <where>  
            <if test="carlineInfoUid != null "> and carline_info_uid = #{carlineInfoUid}</if>
            <if test="createByUid != null  and createByUid != ''"> and create_by_uid = #{createByUid}</if>
            <if test="updateByUid != null  and updateByUid != ''"> and update_by_uid = #{updateByUid}</if>
            <if test="desktopDeviceInformation != null  and desktopDeviceInformation != ''"> and desktop_device_information = #{desktopDeviceInformation}</if>
        </where>
    </select>
    
    <select id="selectTDesktopDeviceByUid" parameterType="Long" resultMap="TDesktopDeviceResult">
        <include refid="selectTDesktopDeviceVo"/>
        where uid = #{uid}
    </select>
        
    <insert id="insertTDesktopDevice" parameterType="TDesktopDevice" useGeneratedKeys="true" keyProperty="uid">
        insert into t_desktop_device
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="carlineInfoUid != null">carline_info_uid,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createByUid != null">create_by_uid,</if>
            <if test="updateByUid != null">update_by_uid,</if>
            <if test="desktopDeviceInformation != null">desktop_device_information,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="carlineInfoUid != null">#{carlineInfoUid},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="createByUid != null">#{createByUid},</if>
            <if test="updateByUid != null">#{updateByUid},</if>
            <if test="desktopDeviceInformation != null">#{desktopDeviceInformation},</if>
         </trim>
    </insert>

    <update id="updateTDesktopDevice" parameterType="TDesktopDevice">
        update t_desktop_device
        <trim prefix="SET" suffixOverrides=",">
            <if test="carlineInfoUid != null">carline_info_uid = #{carlineInfoUid},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="createByUid != null">create_by_uid = #{createByUid},</if>
            <if test="updateByUid != null">update_by_uid = #{updateByUid},</if>
            <if test="desktopDeviceInformation != null">desktop_device_information = #{desktopDeviceInformation},</if>
        </trim>
        where uid = #{uid}
    </update>

    <delete id="deleteTDesktopDeviceByUid" parameterType="Long">
        delete from t_desktop_device where uid = #{uid}
    </delete>

    <delete id="deleteTDesktopDeviceByUids" parameterType="String">
        delete from t_desktop_device where uid in 
        <foreach item="uid" collection="array" open="(" separator="," close=")">
            #{uid}
        </foreach>
    </delete>
</mapper>